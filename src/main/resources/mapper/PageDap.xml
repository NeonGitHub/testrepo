<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.graProject.dao.PageDao">

	<select id="getCaseTotalPage" resultType="int">
		select count(*) from
		treatment_case
	</select>


	<select id="getDataTotalPage" resultType="int">
		select count(*) from
		device_data
	</select>

	<select id="getUserTotalPage" resultType="int">
		select count(*) from
		user_info
	</select>

	<select id="getDiseaseTotalPage" resultType="int">
		select count(*) from
		disease_case
	</select>

	<select id="getFishTotalPage" resultType="int">
		select count(*) from
		fish_type
	</select>
	
	<select id="getBehaveTotalPage" resultType="int">
		select count(*) from
		behave
	</select>

	<select id="getCheckCaseTotalPage" resultType="int">
		select count(*)
		from
		treatment_check
	</select>
</mapper>
    
